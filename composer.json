{
    "name": "anripuankare/atol-client",
    "description": "ATOL API v3/v4 client for php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Agaletskiy",
            "email": "pavel.agaletskiy@lamoda.ru"
        },
        {
            "name": "Sokolov Artem",
            "email": "a.sokolov@reg.ru"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "jms/serializer": "^3.4",
        "symfony/validator": "^4.0 || ^5.0",
        "guzzlehttp/guzzle": "^7.3.0",
        "paillechat/php-enum": "~1.1.0",
        "doctrine/annotations": "^1.7",
        "doctrine/cache": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "friendsofphp/php-cs-fixer": "^2.11"
    },
    "autoload": {
        "psr-4": {
            "Lamoda\\AtolClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lamoda\\AtolClient\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.1"
        }
    }
}
